{{#if showResults}}
  <div class="autocomplete-result-wrapper">
    <div class="autocomplete-result">
      <ul>
        {{#each _items}}
          <li class="autocomplete-item {{extraCls}}" on:el:click="selectItem({{_index}})">
            {{email}}
          </li>
        {{/each}}
      </ul>
    </div>
  </div>
{{/if}}
