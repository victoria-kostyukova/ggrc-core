{{#if isLoading}}
    <spinner-component {toggle}="{isLoading}"></spinner-component>
{{else}}
    {{#if isObject}}
        {{#each data}}
            <div>{{{.}}}</div>
        {{/each}}
    {{else}}
        <div>{{{data}}}</div>
    {{/if}}
{{/if}}
