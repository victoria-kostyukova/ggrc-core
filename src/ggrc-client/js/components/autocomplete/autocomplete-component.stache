<div class="objective-selector autocomplete--wrapper">
  <input type="text"
         data-lookup="{{searchItemsType}}"
         data-no-automap="{{automappingOff}}"
         {{#if disable}}disabled{{/if}}
         class="{{extraCssClass}} {{^if context.isEdit}}autocomplete_wide{{/if}} autocomplete--input"
         placeholder="{{placeholder}}"
         el:value:bind="value">
</div>
