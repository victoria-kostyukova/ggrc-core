{{#each list}}
<li class="{{class.category}}" data-model="true" {{data 'model'}}>
  <a href="{{viewLink}}" class="show-extended">
     <span class="lhs-item lhs-item-medium">
       {{title}}
     </span>
  </a>
</li>
{{/each}}

<li class="spinny">
  <spinner {toggle}="list.is_loading"></spinner>
</li>
